{
  "video": {
    "name": "video",
    "description": "This is an Video block description",
    "schema": {
      "classes": ["inline", "sharepoint", "full-width", "left", "right", "embedded-content", "editorial-width", "no-gutter-block"],
      "contentAtoms": true,
      "poster": {
        "selector": ":scope picture",
        "target": "outerHtml",
        "optional": true
      },
      "posterImg": {
        "selector": ":scope picture img",
        "target": "src",
        "optional": true
      },
      "gif_image": {
        "selector": ".video-gif-image",
        "target": "innerHTML",
        "optional": true
      },
      "local_video": {
        "selector": ".local-video p",
        "target": "textContent",
        "optional": true
      }
    }
  }
}
