{
  "gateway-list": {
    "name": "gateway-list",
    "description": "This is a Gateway List block.",
    "schema": {
      "classes":[],
      "header": {
        "optional": true,
        "selector": "selectRow:header",
        "target": "innerHTML"
      },
      "buttonCollection": {
        "type": "multiple",
        "description": "This is the collection of buttons in the Gateway List",
        "subTarget": ".core-gateway-list > div",
        "items": {
          "link": {
            "selector": ":scope a",
            "target": "outerHTML",
            "optional": true
          },
          "body": {
            "selector": ":scope div:last-child p:nth-child(2)",
            "optional": true,
            "target": "textContent"
          }
        }
      }
    }
  }
}
