{
  "name": "@pfizer/libraryfranklinpfizer",
  "private": true,
  "version": "1.0.0",
  "description": "core-library-version-2",
  "scripts": {
    "test-carousel": "node --max-old-space-size=4096 ./node_modules/.bin/wtr \"./lib/test/blocks/cards/*.test.js\" --node-resolve --port=2000 --coverage --debug --concurrency=5 --plugins ./node_modules/@web/test-runner-commands/filePlugin.js",
    "test": "node --max-old-space-size=4096 ./node_modules/.bin/wtr \"./lib/test/**/*.test.js\" \"./test/**/*.test.js\" --node-resolve --port=2000 --coverage --debug --concurrency=5 --plugins ./node_modules/@web/test-runner-commands/filePlugin.js",
    "test:watch": "npm test -- --watch",
    "lint:js": "eslint .",
    "lint:css": "stylelint lib/blocks/**/*.{css,scss} lib/styles/*.scss --fix",
    "lint:blocks-css-fix": "stylelint lib/blocks/**/*.css --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "sanity-checks": "node ./scripts/sanity-checks.js",
    "generate-icons-json": "node ./scripts/generate-icons-json.js",
    "start-dev": "npm-run-all --parallel watch-css start-server frontend:build-dev watch-lib-src",
    "start-server": "aem up --url https://libraryv2adobepfizer-main-page.web.pfizer/",
    "test:playwright": "playwright test -c ./test/e2e/playwright.config.js",
    "test:playwright:with-debug": "playwright test -c ./test/e2e/playwright.config.js --headed --debug",
    "mat-icons": "node ./scripts/get-icons-material.js",
    "agency-icons": "node ./scripts/get-icons-agency.js",
    "watch-css": "sass --watch ./lib/styles/tokens.scss:./lib/styles/tokens.css ./lib/styles/styles.scss:./lib/styles/styles.css --style compressed & npm run watch-block-parallel",
    "watch-block-parallel": "npm-run-all --parallel watch-css-blocks watch-block-css-and-autoformat",
    "watch-css-blocks": "sass --watch ./lib/blocks --style expanded --no-source-map",
    "watch-block-css-and-autoformat": "onchange --await-write-finish 500 './lib/blocks/**/*.scss' -- sh -c 'npm run lint:blocks-css-fix && echo \"Event: {{event}} File: {{file}} Time: $(date)\"'",
    "build-css-blocks": "sass ./lib/blocks --style expanded --no-source-map",
    "build-css-blocks-and-autoformat": "npm run build-css-blocks && npm run lint:blocks-css-fix",
    "frontend:css-checks": "cd scripts && node css-checks.js",
    "frontend:color-generator": "node ./scripts/color-generator.js",
    "frontend:colors-manual": "cd ./scripts/_manual && node manual.js",
    "frontend:css-from-variables": "node ./scripts/css-from-variables.js",
    "frontend:build-css": "npm run frontend:build-tokens && sass ./lib/styles/styles.scss > ./lib/styles/styles.css --style compressed --no-source-map",
    "frontend:build-tokens": "sass ./lib/styles/tokens.scss ./lib/styles/tokens.css --style compressed --no-source-map",
    "frontend:concat": "node ./lib/build-scripts/concat.js",
    "frontend:build-dev": "node ./lib/build-scripts/build.js dev",
    "frontend:build-prod": "node ./lib/build-scripts/build.js prod",
    "frontend": "npm run frontend:build-tokens && npm run frontend:build-css && npm run frontend:color-generator && npm run build-css-blocks-and-autoformat && npm run frontend:css-checks && npm run frontend:build-prod",
    "poc-figma:colors": "node ./scripts/_wip/figma-api/color/get-figma-colors.js",
    "poc-figma:typography": "node ./scripts/_wip/figma-api/fonts/fonts.js",
    "poc-figma:misc": "node ./scripts/_wip/figma-api/misc/components.js",
    "poc-figma:components": "node ./scripts/_wip/figma-api/components/components.js",
    "add_symlink_library": "",
    "block-list": "node ./scripts/block-list.js",
    "test:doctor-search": "node --max-old-space-size=4096 ./node_modules/.bin/wtr \"./lib/blocks/find-a-doctor/find-doctor-search.test.js\" --node-resolve --port=2000 --coverage --debug",
    "doctor-search": "node ./lib/blocks/find-a-doctor/find-doctor-search-example.mjs",
    "watch-lib-src": "onchange 'lib/scripts/lib-franklin/franklin-lib.js' -- npm run frontend:build-dev",
    "figma-api:sync-figma-to-tokens": "cd scripts/style-dictionary-figma && npm run sync-figma-to-tokens",
    "figma-api:srebuild-tokens": "cd scripts/style-dictionary-figma && npm run rebuild-tokens",
    "figma-api-sync-build": "npm run figma-api:sync-figma-to-tokens && npm run figma-api:srebuild-tokens && cd ./ && npm run frontend:build-tokens",
    "token-var-check": "node ./lib/build-scripts/css-checks/var-check.js",
    "figma-api-complete-check": "npm run figma-api-sync-build && npm run token-var-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/helix-project-boilerplate.git"
  },
  "author": "Adobe",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-project-boilerplate/issues"
  },
  "homepage": "https://github.com/adobe/helix-project-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/eslint-parser": "7.22.9",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@material-design-icons/svg": "^0.14.10",
    "@open-wc/testing": "^4.0.0",
    "@playwright/test": "^1.38.0",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-mocha": "^0.9.0",
    "chai": "4.3.7",
    "chai-a11y-axe": "^1.5.0",
    "css-tree": "^2.3.1",
    "ejs": "^3.1.9",
    "esbuild": "^0.24.2",
    "eslint": "8.46.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-diff": "^29.3.1",
    "jest-matcher-utils": "^29.3.1",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "playwright": "^1.38.0",
    "postcss": "^8.4.27",
    "sass": "^1.64.2",
    "sinon": "15.0.1",
    "stylelint": "15.10.2",
    "stylelint-config-standard-scss": "^10.0.0"
  },
  "engines": {
    "node": "16.20.2"
  },
  "dependencies": {
    "@open-wc/testing-helpers": "^3.0.1",
    "dotenv": "^16.4.5",
    "js-beautify": "^1.15.1",
    "terser": "^5.31.3"
  }
}
