{
  "header": {
    "schema": {
      "classes": [],
      "socialSection": {
        "optional": true,
        "linkItemCollection": {
          "type": "multiple",
          "subTarget": ".section-social-icons ul > li",
          "optional": true,
          "items": {
            "href": {
              "selector": ":scope > a",
              "target": "href"
            },
            "iconClass": {
              "selector": ":scope > a > span",
              "target": "className"
            }
          }
        }
      },
      "rightButtonHref": {
        "selector": ".section-button-right a",
        "target": "href",
        "optional": true
      },
      "rightButtonLink": {
        "selector": ".section-button-right a",
        "target": "textContent",
        "optional": true
      },
      "leftButtonIcon": {
        "selector": ".section-button-left a span",
        "target": "className",
        "optional": true
      },
      "rightButtonIcon": {
        "selector": ".section-button-right a span",
        "target": "className",
        "optional": true
      },
      "leftButtonHref": {
        "selector": ".section-button-left a",
        "target": "href",
        "optional": true
      },
      "leftButtonLink": {
        "selector": ".section-button-left a",
        "target": "textContent",
        "optional": true
      },
      "splitterButton": {
        "selector": ".section-splitter-button a",
        "target": "outerHTML",
        "optional": true
      },
      "bottomDropdownTop": {
        "selector": ".section-button-right a",
        "target": "href",
        "optional": true
      },
      "bottomRightLinksCollection": {
        "type": "multiple",
        "description": "This is a collection of links on the bottom right of the menu.",
        "subTarget": ".section-bottom-right-links ul li",
        "optional": true,
        "items": {
          "label": {
            "selector": "a",
            "target": "textContent"
          },
          "href": {
            "selector": "a",
            "target": "href"
          }
        }
      },
      "onlyOneBottomLink": {
        "selector": ".section-dropdown-bottom > div > a",
        "target": "outerHTML",
        "optional": true
      },
      "onlyOneTopLink": {
        "selector": ".section-dropdown-top > div > a",
        "target": "outerHTML",
        "optional": true
      },
      "dropdownTop": {
        "selector": ".section-dropdown-top p",
        "target": "textContent",
        "optional": true
      },
      "dropdownBottom": {
        "selector": ".section-dropdown-bottom p",
        "target": "textContent",
        "optional": true
      },
      "primaryLogoClass": {
        "selector": ".section-logo a span.icon",
        "target": "className"
      },
      "primaryLogoClassReversed": {
        "selector": ".section-logo-reversed a span.icon",
        "target": "className"
      },
      "dropdownTopLinksCollection": {
        "type": "multiple",
        "description": "This is a collection of dropdown menu items at the top of the bottom of the menu",
        "subTarget": ".section-dropdown-top ul > li",
        "optional": true,
        "items": {
          "href": {
            "selector": "a",
            "target": "href"
          },
          "label": {
            "selector": "a",
            "target": "textContent"
          }
        }
      },
      "dropdownBottomLinksCollection": {
        "type": "multiple",
        "description": "This is a collection of dropdown menu items at the bottom of the bottom of the menu",
        "subTarget": ".section-dropdown-bottom ul > li",
        "optional": true,
        "items": {
          "href": {
            "selector": "a",
            "target": "href"
          },
          "label": {
            "selector": "a",
            "target": "textContent"
          }
        }
      },
      "logoBottom": {
        "selector": ".section-logo-bottom a span.icon",
        "target": "className",
        "optional": true
      },
      "topMenuItemCollection": {
        "type": "multiple",
        "description": "This is a collection of top-level menu items. Each top-level menu item has a list of sub-menu items.",
        "subTarget": ".section-top-menu > div > ul > li",
        "items": {
          "multiPageVariantL1MenuLabels": {
            "selector": "span",
            "target": "outerHTML",
            "optional": true
          },
          "staticVariantL1MenuLinks": {
            "selector": "a",
            "target": "outerHTML",
            "optional": true
          },
          "multiPageMenuL2SubLinks": {
            "selector": ":scope > ul",
            "target": "outerHTML",
            "optional": true
          }
        }
      },
      "hvaCta": {
        "selector": ".section-hva-cta a",
        "target": "outerHTML",
        "description": "Header HVA CTA",
        "optional": true
      }
    }
  }
}
