        <div class="core-hero-body-locator-dropdown"></div>
        <label>{{ locatorInputPlaceholder }}</label>
        <p class="core-hero-body-locator-error-message hidden">{{ locatorInputErrorMessage }}</p>
        <div class="core-hero-body-locator-submit">
          <p>
            <em><a href="{{ locatorSubmitUrl }}"><span class="icon icon-lib-map-pin"></span>{{ locatorSubmitText }}</a></em>
          </p>
