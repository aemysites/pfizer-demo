{
  "footer": {
    "name": "footer",
    "description": "This is an footer block",
    "schema": {
      "disclaimer": {
        "selector": ".section-disclaimer > div:last-of-type",
        "target": "innerHTML",
        "optional": true
      },
      "linkItemCollection": {
        "classes": [],
        "type": "multiple",
        "description": "This is a collection of accordion items. Each item consists of a header and a body.",
        "subTarget": ".section-navigation > div > ul > li",
        "optional": true,
        "items": {
          "title": {
            "selector": ":scope > span",
            "target": "textContent",
            "optional": true
          },
          "menu": {
            "selector": ":scope > ul, :scope > a",
            "target": "outerHTML"
          }
        }
      },
      "socialSection": {
        "optional": true,
        "linkItemCollection": {
          "type": "multiple",
          "subTarget": ".section-social ul > li",
          "optional": true,
          "items": {
            "href": {
              "selector": ":scope > a",
              "target": "href"
            },
            "iconClass": {
              "selector": ":scope > a > span",
              "target": "className"
            }
          }
        }
      },
      "languageSection": {
        "languageSelector": {
          "selector": ".section-language div:nth-of-type(2)",
          "target": "textContent",
          "optional": true
        },
        "optional": true
      },
      "utilityLinksSection": {
        "linkItemCollection": {
          "type": "multiple",
          "subTarget": ".section-utility-links ul > li",
          "items": {
            "href": {
              "selector": ":scope > a",
              "target": "href"
            },
            "text": {
              "selector": ":scope > a",
              "target": "textContent"
            }
          }
        }
      },
      "logo": {
        "optional": true,
        "href": {
          "selector": ".section-logo  a",
          "target": "href",
          "optional": true
        },
        "iconClass": {
          "selector": ".section-logo a span",
          "target": "className",
          "optional": true
        }
      },
      "copyRightText": {
        "selector": ".section-copyright > div:last-of-type",
        "target": "innerHTML"
      }
    }
  }
}
